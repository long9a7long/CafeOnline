                                    <a class="module-cancel" href="#"><i class="fa fa-times"></i></a>
                                </div>
                            </div>
                        </div>*@
                    </div>
                    <!-- /.navbar-collapse -->
                </div>
                <!-- /.container-fluid -->
            </nav>
        </header>


        <section id="slider" class="slider slide-overlay-dark">

            <!-- START REVOLUTION SLIDER 5.0 -->
            @RenderSection("slider", false)
