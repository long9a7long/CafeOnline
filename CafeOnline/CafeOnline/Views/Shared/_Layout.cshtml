    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <meta name="description" content="Elegant Restaurant & Cafe Html5 Template by zytheme">
    <link href="~/Assets/display/images/favicon/favicon.png" rel="icon">

    <!-- Fonts
    ============================================= -->
    <link href="https://fonts.googleapis.com/css?family=Open+Sans" rel="stylesheet">


    <!-- Stylesheets
    ============================================= -->
    <link href="~/Assets/display/css/external.css" rel="stylesheet">
    <link href="~/Assets/display/css/bootstrap.min.css" rel="stylesheet">
